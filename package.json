{
  "name": "@jhae/stylelint-rule-tester",
  "description": "A Stylelint rule tester for Jest.",
  "version": "0.0.0",
  "main": "./dist/rule-test.class.js",
  "types": "./types/rule-test.class.d.ts",
  "author": {
    "name": "JHAE",
    "url": "https://github.com/jhae-de"
  },
  "repository": {
    "url": "https://github.com/jhae-de/stylelint-rule-tester.git",
    "type": "git"
  },
  "homepage": "https://github.com/jhae-de/stylelint-rule-tester",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npm run-script lint:prettier && npm run-script lint:eslint",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier . --check",
    "test": "jest",
    "test:coverage": "npm run-script test -- --coverage",
    "test:watch": "npm run-script test -- --watchAll",
    "test:watch:coverage": "npm run-script test:watch -- --coverage"
  },
  "devDependencies": {
    "@eslint/js": "^9.11",
    "@tsconfig/node20": "^20.1",
    "@types/jest": "^29.5",
    "eslint": "^9.11",
    "eslint-config-prettier": "^9.1",
    "eslint-plugin-prettier": "^5.2",
    "globals": "^15.9",
    "jest": "^29.7",
    "prettier": "^3.3",
    "ts-jest": "^29.2",
    "typescript": "^5.6",
    "typescript-eslint": "^8.7"
  },
  "peerDependencies": {
    "stylelint": "^16.9"
  },
  "keywords": [
    "stylelint",
    "stylelint-scss",
    "stylelintrc",
    "configuration",
    "config",
    "rule",
    "rules",
    "test",
    "jest",
    "ts-jest",
    "helper"
  ]
}
